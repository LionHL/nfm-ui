/* 头部样式 相对semantic做样式调整 */

.nfm.header {
  @mixin positions relative;
  padding: 4px 10px;
  background: #191919;
  border-bottom: 2px solid #444;
  z-index: 9;
}

.nfm.menu {
  margin: 0;
  border: 0;
  border-radius: 0;
  .item {
    color: rgb(126, 127, 128);
  }
  &> .item:first-child {
    color: #fff;
  }
  a.item:hover {
    color: #fff !important;
  }
}
