/* 自定义table样式 */

.nfm-table {
  border-radius: 5px;
  overflow: hidden;
  z-index: 2;
  position: relative;
}

.nfm.table {
  margin: 0;
  td {
    font-size: 13px;
  }
  .ui.checkbox {
    vertical-align: text-top;
  }
  .ui.checkbox label {
    padding-left: 0;
  }
  .ui.input input {
    padding: 5px 8px;
  }
}